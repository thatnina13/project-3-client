{{#each rsvps as |rsvp|}}
  <section data-id="{{rsvp.party}}" class="party-log" >
    <h6>Date:</h6>
    <p>
      {{rsvp.party.date}}
    </p>
    <h6>Title:</h6>
    <p>
        {{rsvp.party.title}}
    </p>
    <h6>Details:</h6>
    <p>
        {{rsvp.party.details}}
    </p>
  </section>

{{/each}}
