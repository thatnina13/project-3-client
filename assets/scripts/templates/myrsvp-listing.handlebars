{{#each rsvps as |rsvp|}}
  <section data-id="{{rsvp.party}}" class="party-log" >
    <div class="card-deck">
        <div class="card" style="width:600px">
          <img class="card-img-top" src="https://media.giphy.com/media/3owzW9EZI8M3sz9s5O/giphy.gif" alt="Card image">
          <div class="card-body">
    <h6>Date:</h6>
    <p>
      {{rsvp.party.date}}
    </p>
    <h6>Title:</h6>
    <p>
        {{rsvp.party.title}}
    </p>
    <h6>Details:</h6>
    <p>
        {{rsvp.party.details}}
    </p>
  </div>
</div>
</div>
  </section>

{{/each}}
